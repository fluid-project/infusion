{
    "name": "infusion",
    "description": "Infusion is an application framework for developing flexible stuff with JavaScript",
    "version": "3.0.0",
    "author": "Fluid Project",
    "bugs": "http://issues.fluidproject.org/browse/FLUID",
    "homepage": "http://www.fluidproject.org/",
    "license": "(BSD-3-Clause OR ECL-2.0)",
    "keywords": [
        "infusion",
        "framework",
        "application",
        "fluid",
        "IoC",
        "Inversion of Control",
        "MVC",
        "evented"
    ],
    "repository": "git://github.com/fluid-project/infusion.git",
    "main": "./src/module/fluid.js",
    "scripts": {
        "prepare": "run-s deps build:sass",
        "prepublishOnly": "npm run test:bundles",
        "pretest": "rimraf reports/* coverage/*",
        "test": "run-s test:browser test:node",
        "test:browser": "testem ci --file tests/testem.js",
        "test:bundles": "run-s test:bundles:*",
        "test:bundles:prepare": "npm run build:dists",
        "test:bundles:run": "testem ci --file tests/bundle-tests/testem.js",
        "test:node": "nyc node tests/node-tests/basic-node-tests.js",
        "test:report": "nyc report -r text-summary -r html -r json",
        "posttest": "npm run test:report",
        "build:all": "grunt",
        "build:all:source": "grunt --source=true",
        "build:custom": "grunt custom",
        "build:custom:source": "grunt custom --source=true",
        "build:dists": "grunt buildDists",
        "build:sass": "grunt buildSass",
        "deps": "grunt loadDependencies",
        "lint": "fluid-lint-all",
        "start": "serve",
        "watch:sass": "grunt watch:buildSass"
    },
    "engines": {
        "node": ">=12"
    },
    "browserslist": [
        "extends browserslist-config-fluid"
    ],
    "dependencies": {
        "fluid-resolve": "1.3.0"
    },
    "devDependencies": {
        "@fluid-project/hyphenation-patterns": "0.2.2-dev.20181115T221631Z.b2d0651",
        "browserslist-config-fluid": "0.1.0",
        "eslint-config-fluid": "2.0.0",
        "eslint-plugin-jsdoc": "30.7.3",
        "fluid-lint-all": "1.0.2",
        "fluid-testem": "2.1.13",
        "grunt": "1.3.0",
        "grunt-contrib-clean": "2.0.0",
        "grunt-contrib-compress": "1.6.0",
        "grunt-contrib-copy": "1.0.0",
        "grunt-contrib-watch": "1.1.0",
        "grunt-dart-sass": "1.1.3",
        "grunt-modulefiles": "0.4.0",
        "grunt-shell": "3.0.1",
        "grunt-terser": "1.0.0",
        "hypher": "0.2.5",
        "jquery": "3.3.1",
        "jquery-mockjax": "2.6.0",
        "jquery-simulate": "1.0.2",
        "jquery-ui": "1.12.1",
        "jquery-ui-touch-punch": "0.2.3",
        "jquery.scrollto": "2.1.2",
        "lodash": "4.17.20",
        "ncp": "2.0.0",
        "normalize.css": "8.0.1",
        "npm-run-all": "4.1.5",
        "nyc": "15.1.0",
        "open-dyslexic": "1.0.3",
        "opensans-webkit": "1.1.0",
        "rimraf": "3.0.2",
        "roboto-fontface": "0.10.0",
        "serve": "11.3.2",
        "sinon": "9.1.0",
        "testem": "3.2.0",
        "url-polyfill": "1.1.10"
    }
}
