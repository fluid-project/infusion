{
    "name": "infusion",
    "description": "Infusion is an application framework for developing flexible stuff with JavaScript",
    "version": "3.0.0",
    "author": "Fluid Project",
    "bugs": "http://issues.fluidproject.org/browse/FLUID",
    "homepage": "http://www.fluidproject.org/",
    "license": "(BSD-3-Clause OR ECL-2.0)",
    "keywords": [
        "infusion",
        "framework",
        "application",
        "fluid",
        "IoC",
        "Inversion of Control",
        "MVC",
        "evented"
    ],
    "repository": "git://github.com/fluid-project/infusion.git",
    "main": "./src/module/fluid.js",
    "scripts": {
        "prepare": "npm run loadDependencies && npm run buildSass",
        "prepublishOnly": "npm run buildDists && npm run test:browserBundles",
        "pretest": "node node_modules/rimraf/bin.js reports/* coverage/*",
        "test": "npm run test:browser && npm run test:node",
        "test:browser": "node node_modules/testem/testem.js ci --file tests/testem.js",
        "test:browserBundles": "node node_modules/testem/testem.js ci --file tests/bundle-tests/testem.js",
        "test:node": "node node_modules/nyc/bin/nyc.js node tests/node-tests/basic-node-tests.js",
        "posttest": "node node_modules/nyc/bin/nyc.js report -r text-summary -r html -r json",
        "buildDists": "grunt buildDists",
        "buildSass": "grunt buildSass",
        "buildLatest": "grunt --source && grunt copy:all && npm run build:manifest",
        "buildManifest": "node bin/build-manifest.js",
        "loadDependencies": "grunt loadDependencies",
        "lint": "grunt lint"
    },
    "engines": {
        "node": ">=12"
    },
    "browserslist": [
        "extends browserslist-config-fluid"
    ],
    "dependencies": {
        "fluid-resolve": "1.3.0"
    },
    "devDependencies": {
        "@fluid-project/hyphenation-patterns": "0.2.2-dev.20181115T221631Z.b2d0651",
        "browserslist-config-fluid": "0.1.0",
        "eslint-config-fluid": "1.3.0",
        "eslint-plugin-markdown": "1.0.2",
        "fast-glob": "3.2.4",
        "fluid-grunt-lint-all": "1.0.8",
        "fluid-testem": "2.1.13",
        "grunt": "1.3.0",
        "grunt-contrib-clean": "2.0.0",
        "grunt-contrib-compress": "1.6.0",
        "grunt-contrib-concat": "1.0.1",
        "grunt-contrib-copy": "1.0.0",
        "grunt-contrib-uglify": "4.0.1",
        "grunt-contrib-watch": "1.1.0",
        "grunt-dart-sass": "1.1.3",
        "grunt-modulefiles": "0.4.0",
        "grunt-shell": "3.0.1",
        "hypher": "0.2.5",
        "jquery": "3.3.1",
        "jquery-mockjax": "2.5.1",
        "jquery-simulate": "1.0.2",
        "jquery-ui": "1.12.1",
        "jquery-ui-touch-punch": "0.2.3",
        "jquery.scrollto": "2.1.2",
        "lodash": "4.17.19",
        "ncp": "2.0.0",
        "normalize.css": "8.0.1",
        "nyc": "15.1.0",
        "open-dyslexic": "1.0.3",
        "opensans-webkit": "1.1.0",
        "rimraf": "3.0.2",
        "roboto-fontface": "0.10.0",
        "sinon": "9.0.2",
        "testem": "3.2.0",
        "url-polyfill": "1.1.8"
    }
}
